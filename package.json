{
  "name": "transaction-pilot",
  "version": "1.0.0",
  "description": "Chrome extension to simulate dApp interactions and record transactions",
  "main": "index.js",
  "repository": "https://github.com/gnosis/transaction-pilot.git",
  "author": "Jan-Felix <jan-felix.schwarz@gnosis.io>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "rimraf public/build",
    "build": "NODE_ENV=production node esbuild.js",
    "dev": "NODE_ENV=development node esbuild.js",
    "check": "yarn check:ts && yarn check:prettier && yarn check:lint && yarn check:spelling",
    "check:ts": "tsc --noEmit",
    "check:lint": "eslint src --ext .ts,.tsx",
    "check:prettier": "prettier \"src/**/*.(ts|tsx)\" --list-different",
    "check:spelling": "cspell \"**\"",
    "fix": "yarn fix:prettier && yarn fix:lint",
    "fix:prettier": "prettier \"src/**/*.(ts|tsx)\" --write",
    "fix:lint": "eslint src --ext .ts --fix"
  },
  "devDependencies": {
    "@gnosis.pm/safe-core-sdk": "^1.1.1",
    "@gnosis.pm/safe-service-client": "^1.0.1",
    "@types/chrome": "^0.0.164",
    "@types/events": "^3.0.0",
    "@types/react": "^17.0.35",
    "@types/react-blockies": "^1.4.1",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@walletconnect/ethereum-provider": "^1.6.6",
    "@walletconnect/types": "^1.6.6",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "cspell": "^5.13.0",
    "esbuild": "^0.13.14",
    "esbuild-css-modules-plugin": "^2.0.9",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "ethers": "^5.5.1",
    "events": "^3.3.0",
    "node-stdlib-browser": "^1.1.0",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-blockies": "^1.4.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-select": "^5.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.2",
    "typescript-plugin-css-modules": "^3.4.0"
  }
}
